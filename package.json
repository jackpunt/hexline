{
  "name": "@thegraid/hexline",
  "version": "0.2.2",
  "browser": "true",
  "scripts": {
    "fixnpm": "npm cache validate",
    "uncache": "rm -rf .angular/cache",
    "make-proto": "bin/make-patch",
    "clean": "rm -rf dist/*",
    "make": "rm -rf dist/*; bin/make-patch; tsc; date",
    "instClient": "npm i ../../ts/wspbclient/thegraid-wspbclient*.tgz && bin/uncache; date",
    "instEaseljs": "bin/uncache && npm i ../../ts/createjs-lib/thegraid-easeljs-lib*.tgz",
    "rmall": "aws s3 rm s3://hexline.thegraid.com/ --recursive",
    "sync": "aws s3 sync dist/hexline s3://hexline.thegraid.com",
    "prod": "ng build hexline --configuration production",
    "build": "npm run clean; npm run make-proto; npm run tsc",
    "newhex": "npm i ../../ts/hexlib/thegraid-hexlib-*.tgz && rm -rf .angular/cache; date",
    "newlib": "npm i ../../ts/createjs-lib/thegraid-easeljs-lib-*.tgz && rm -rf .angular/cache; date",
    "tsc": "tsc --build tsconfig.app.json",
    "ng": "ng",
    "start": "ng serve",
    "ngbuild": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "note1": "for file-link dependency, disable cache; ng config cli.cache.enabled false",
  "note2": "but_even_so, ng/webpack will load createjs TWICE; so: cant't use links",
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@thegraid/common-lib": "^1.3.24",
    "@thegraid/easeljs-lib": "^1.3.11",
    "@thegraid/easeljs-module": "^1.1.9",
    "@thegraid/ezpromise": "^1.0.10",
    "@thegraid/hexlib": "^1.1.9",
    "@thegraid/wspbclient": "^1.4.26",
    "@types/wicg-file-system-access": "^2020.9.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "file-loader": "^6.2.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "require-context": "^1.1.0",
    "typescript": "~4.8.4"
  }
}