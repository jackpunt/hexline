{
  "name": "hexline",
  "version": "0.0.0",
  "scripts": {
    "make-proto": "npm run protots; npm run patch-proto; tsc -p src/proto",
    "make-patch": "cp src/proto/HgProto.ts src/proto/HgProto-patched.ts; npm run protots; (cd src/proto; diff -c HgProto.ts HgProto-patched.ts > HgProto.patch); npm run patch-proto",
    "patch-proto": "patch src/proto/HgProto.ts < src/proto/HgProto.patch",
    "protots": "protoc -I src/proto --ts_out=src/proto src/proto/*.proto",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "for file:link dependency, disable cache": "ng config cli.cache.enabled false",
  "but even so, ng/webpack will load createjs TWICE": "can't use links",
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@thegraid/common-lib": "^1.3.21",
    "@thegraid/easeljs-lib": "^1.1.3",
    "@thegraid/easeljs-module": "^1.1.4",
    "@thegraid/ezpromise": "^1.0.10",
    "@thegraid/wspbclient": "^1.4.6",
    "@types/wicg-file-system-access": "^2020.9.5",
    "moment": "^2.29.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "~13.2.0",
    "@angular/cli": "~13.2.0",
    "@angular/compiler-cli": "~13.2.0",
    "@types/google-protobuf": "^3.15.5",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
